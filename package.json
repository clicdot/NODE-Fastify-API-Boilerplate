{
  "name": "node-microservice-boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build:dev": "pm2 start process.json",
    "build:m360": "pm2 start process.json --env m360",
    "build:uat": "pm2 start process.json --env uat",
    "build:prod": "pm2 start process.json --env production",
    "lint": "semistandard | snazzy",
    "start": "NODE_ENV=dev nodemon -L -w ./src ./src/server.js",
    "test": "npm run lint && npm run test:unit",
    "test:coverage": "npm run test:unit -- --cov --coverage-report=text --coverage-report=html",
    "test:load": "artillery run ./test-load/root.v1.yml",
    "test:unit": "tap ./test/**/*.test.js"
  },
  "author": "Philip Choi",
  "license": "ISC",
  "dependencies": {
    "app-root-path": "^2.2.1",
    "aws-serverless-express": "^3.3.6",
    "fastify": "^2.4.1",
    "fastify-autoload": "^0.8.0",
    "fastify-blipp": "^2.0.0",
    "fastify-cli": "^1.0.0",
    "fastify-compress": "^0.10.0",
    "fastify-cors": "^2.1.3",
    "fastify-elasticsearch": "^0.4.0",
    "fastify-env": "^0.6.2",
    "fastify-helmet": "^3.0.0",
    "fastify-jwt": "^4.2.0",
    "fastify-mysql": "^0.3.0",
    "fastify-plugin": "^1.5.0",
    "fastify-redis": "^3.1.0",
    "fastify-static": "^2.4.0",
    "fastify-swagger": "^2.4.0",
    "fastify-swaggergen": "^1.1.1",
    "fs": "0.0.1-security",
    "fs-extra": "^8.0.1",
    "pino": "^5.12.6",
    "swagger-ui-dist": "^3.22.2",
    "uuid": "^3.3.2",
    "x-xss-protection": "^1.1.0"
  },
  "devDependencies": {
    "artillery": "^1.6.0-28",
    "cheerio": "^1.0.0-rc.3",
    "form-data": "^2.3.3",
    "nock": "^10.0.6",
    "nodemon": "^1.19.0",
    "pm2": "^3.5.0",
    "pre-commit": "^1.2.2",
    "semistandard": "^13.0.1",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "tap": "^14.1.11"
  }
}
